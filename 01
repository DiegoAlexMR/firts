#include <stdio.h>

/*Soma 
Subitração
Multiplicação
Divisão
soma
porcentagem*/



int main ()
{
    
    printf("a= %d - b= %d, resultado=%d\n", 5, 3, 5 - 3);
    printf("a= %d * b= %d, resultado=%d\n", 5, 3, 5 * 3);
    printf("a= %d / b= %d, resultado=%d\n", 5, 3, 5 / 3);
    printf("a= %d + b= %d, resultado=%d\n", 5, 3, 5 + 3);
    printf("a= %d %% b= %d, resultado=%d\n", 5, 3, 5 % 3);

}
